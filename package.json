{
  "name": "billboard-app",
  "version": "1.0.0",
  "description": "Billboard advertising app",
  "main": "server.js",
  "scripts": {
    "start:react": "cd frontend && react-scripts start",
    "start:server": "node server.js",
    "start:server:dev": "nodemon server.js",
    "client:install": "npm --prefix frontend install",
    "start:client": "npm --prefix frontend start",
    "dev": "concurrently \"npm: start:server:dev\" \"npm: start:client\"",
    "build": "cd frontend && react-scripts build",
    "test": "cd frontend && react-scripts test",
    "eject": "cd frontend && react-scripts eject",
    "check:react": "node ./scripts/check-react.js",
    "clean:reinstall": "rimraf node_modules package-lock.json frontend/node_modules frontend/package-lock.json && npm install && npm --prefix frontend install",
    "create-admin": "node scripts/create-admin.js"
  },
  "dependencies": {
    "axios": "^1.3.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^6.20.0",
    "mongoose": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.11",
    "nodemon": "^2.0.20",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.0.1",
    "rimraf": "^5.0.0"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
